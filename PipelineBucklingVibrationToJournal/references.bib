	isbn = {0022-460X},
	journal = {Journal of Sound and Vibration},
	number = {3},
	pages = {481--498},
	title = {Natural frequencies of beams under tensile axial loads},
	url = {https://www.sciencedirect.com/science/article/pii/0022460X9090663K},
	volume = {142},
