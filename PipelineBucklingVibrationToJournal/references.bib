	pages = {102407},
	title = {Dynamics of a Pipeline Span on an Elastic Seabed},
	url = {https://www.sciencedirect.com/science/article/pii/S0141118720309664},
	volume = {111},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0141118720309664},
